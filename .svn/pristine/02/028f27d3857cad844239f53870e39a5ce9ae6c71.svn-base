			<xs:element name="adults" type="IntegerGreaterThanOne" minOccurs="0"/>
			<xs:element name="guests" type="IntegerGreaterThanOne"/>
			<xs:element name="note" type="xs:string" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="MinimumAgeHouseRule">
		<xs:sequence>
			<xs:element name="age" type="xs:int"/>
			<xs:element name="note" type="xs:string" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="Money" final="extension restriction">
		<xs:annotation>
			<xs:documentation xml:lang="en">Represents an amount of money.</xs:documentation>
		</xs:annotation>
		<xs:simpleContent>
			<xs:extension base="xs:decimal">
				<xs:attribute name="currency" type="CurrencyType">
					<xs:annotation>
						<xs:documentation xml:lang="en">ISO currency code.</xs:documentation>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	<xs:complexType name="NearestPlace">
		<xs:annotation>
			<xs:documentation xml:lang="en">Information regarding a place of interest nearby to a vacation rental.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="distance" type="xs:decimal" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">The distance to the place of interest, expressed in the given DistanceUnit.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="distanceUnit" type="DistanceUnit" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">The unit of distance.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="name" type="Note" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">The name of the place of interest.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="placeType" use="required">
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:enumeration value="AIRPORT"/>
					<xs:enumeration value="BAR"/>
					<xs:enumeration value="BEACH"/>
					<xs:enumeration value="FERRY"/>
					<xs:enumeration value="GOLF"/>
					<xs:enumeration value="HIGHWAY"/>
					<xs:enumeration value="RESTAURANT"/>
					<xs:enumeration value="SKI"/>
					<xs:enumeration value="TRAIN"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="NightlyRates">
