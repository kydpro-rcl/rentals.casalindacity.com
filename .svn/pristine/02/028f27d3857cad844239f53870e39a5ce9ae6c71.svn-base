	<xs:complexType name="LatLng">
		<xs:annotation>
			<xs:documentation xml:lang="en">A latitude/longitude coordinate pair.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="latitude">
				<xs:simpleType>
					<xs:annotation>
						<xs:documentation xml:lang="en">The latitude coordinate.  Latitude values must be between -90 and 90 degress.</xs:documentation>
					</xs:annotation>
					<xs:restriction base="xs:decimal">
						<xs:minInclusive value="-90.0"/>
						<xs:maxInclusive value="90.0"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="longitude">
				<xs:simpleType>
					<xs:annotation>
						<xs:documentation xml:lang="en">The longitude coordinate.  Longitude values must be between -180 and 180 degrees.</xs:documentation>
					</xs:annotation>
					<xs:restriction base="xs:decimal">
						<xs:minInclusive value="-180.0"/>
						<xs:maxInclusive value="180.0"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="Link">
		<xs:annotation>
			<xs:documentation xml:lang="en">A hypermedia link.</xs:documentation>
		</xs:annotation>
		<xs:sequence/>
		<xs:attribute name="href" type="xs:anyURI" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="en">The linked URL.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="rel" type="xs:string">
			<xs:annotation>
				<xs:documentation xml:lang="en">The relationship of the linked resource to the resource from which the link originates.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="locale" type="xs:string" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="en">The locale for the linked URL.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="Listing">
		<xs:annotation>
			<xs:documentation xml:lang="en">A Listing is an advertisement for a vacation rental - it contains the details of the rental
 property that are presented to travelers.

 Listings encompass one or more Units.  The Listing represents data about the property as a
 whole - the physical location and attributes of the property as a whole, for example.  Each
 Unit represents a single rentable unit of the property, and describes the particulars of that
 unit.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="Entity">
				<xs:sequence>
					<xs:element name="externalId" nillable="false">
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:maxLength value="255"/>
								<xs:minLength value="1"/>
								<xs:pattern value="[^/]*"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="active" type="xs:boolean" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">Indicates whether the Listing is active or inactive.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="adContent" type="AdContent">
						<xs:annotation>
							<xs:documentation xml:lang="en">Descriptive content about the vacation rental.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="canonicalLinks" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">INTERNAL USE ONLY</xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:sequence>
								<xs:element name="canonicalLink" type="Link" maxOccurs="unbounded"/>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<xs:element name="distributionProduct" type="xs:string" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">INTERNAL USE ONLY</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="featureValues" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">Feature Values represent the normalized property features that the Advertiser can select
 for the listing.</xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:sequence>
								<xs:element name="featureValue" type="ListingFeatureValue" minOccurs="0" maxOccurs="unbounded"/>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<xs:element name="location" type="Location">
						<xs:annotation>
							<xs:documentation xml:lang="en">Information about the location of the vacation rental.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element ref="images" minOccurs="0"/>
					<xs:element name="matterportUrl" type="xs:string" minOccurs="0"/>
					<xs:element name="units">
						<xs:annotation>
							<xs:documentation xml:lang="en">Listings are composed of one or more Units - each Unit is an independently rentable unit
 with its own rates and availability, in the same physical location (e.g. a duplex, or
 several cottages on the same property).</xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:sequence>
								<xs:element name="unit" type="Unit" maxOccurs="unbounded"/>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
