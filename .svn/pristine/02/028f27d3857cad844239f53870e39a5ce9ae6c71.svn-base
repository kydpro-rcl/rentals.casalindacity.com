					<xs:sequence>
						<xs:element name="text" type="Text" maxOccurs="unbounded"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="PaymentSchedulePayment">
		<xs:sequence>
			<xs:element name="days" minOccurs="0">
