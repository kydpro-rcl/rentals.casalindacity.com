					<xs:sequence>
						<xs:element name="override" type="AmountNightlyOverrideForNightlyRates" maxOccurs="2000"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="sat" type="AmountDecimalValueType"/>
			<xs:element name="sun" type="AmountDecimalValueType"/>
			<xs:element name="thu" type="AmountDecimalValueType"/>
			<xs:element name="tue" type="AmountDecimalValueType"/>
			<xs:element name="wed" type="AmountDecimalValueType"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="Note">
		<xs:annotation>
			<xs:documentation xml:lang="en">A piece of localizable text.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="texts">
				<xs:annotation>
					<xs:documentation xml:lang="en">The list of variant of this text, by Locale.</xs:documentation>
				</xs:annotation>
