			<xs:element name="amount" type="AmountDecimalValueType"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="PaymentScheduleRequiresAmountPercentOfTotalBooking">
		<xs:sequence>
			<xs:element name="percent" type="xs:decimal"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="PaymentScheduleRequiresAmountRemainder">
		<xs:sequence/>
	</xs:complexType>
	<xs:complexType name="PercentNightlyOverride">
