			<xs:element name="ownerListingStory" type="Note" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">The "story" of the property, how the owner came to own the property, and the role the
 property has played in his/her life.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="propertyName" type="Note" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">The name of the property.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="uniqueBenefits" type="Note" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">A description of the unique benefits offered by this property over others.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="whyPurchased" type="Note" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">A description of why the owner purchased this particular property.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="yearPurchased" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">The year in which the property was purchased.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
