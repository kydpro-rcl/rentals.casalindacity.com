				</xs:simpleType>
			</xs:element>
			<xs:element name="dueType" type="PaymentSchedulePaymentDueType"/>
			<xs:element name="externalId" type="LodgingRateExternalId" minOccurs="0"/>
			<xs:choice>
				<xs:element name="requiresFlatAmountOf" type="PaymentScheduleRequiresAmountFlat" minOccurs="0"/>
				<xs:element name="requiresPercentOfTotalBooking" type="PaymentScheduleRequiresAmountPercentOfTotalBooking" minOccurs="0"/>
				<xs:element name="requiresRemainder" type="PaymentScheduleRequiresAmountRemainder" minOccurs="0"/>
			</xs:choice>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="PaymentScheduleRequiresAmountFlat">
