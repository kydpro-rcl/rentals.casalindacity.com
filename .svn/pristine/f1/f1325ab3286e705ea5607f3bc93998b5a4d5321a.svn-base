
    
    <!-- Form Validation Script // requires formvalidation.js -->
			function check(cform) {
				var succ = checkForm(cform);
						if (!succ) {
						  document.getElementById('globalmsg').innerHTML = 'The form contains errors, please correct and resubmit.';
						  document.getElementById('globalmsg').style.display = 'block';
						}
					 else {
						document.getElementById('dobm').innerHTML="* Incorrect D.O.B.";
						return(false);
					}
				return(succ);
			}