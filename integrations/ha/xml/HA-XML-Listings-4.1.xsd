				<xs:sequence>
					<xs:element name="percent" type="xs:decimal"/>
					<xs:choice>
						<xs:element name="appliesPerNight" type="AmountAppliesPerNight"/>
						<xs:element name="appliesPerStay" type="AmountAppliesPerStay"/>
