					<xs:sequence>
						<xs:element name="rule" type="TaxRuleFlat" maxOccurs="15"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="percentOfFeesTaxRules" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="rule" maxOccurs="15">
							<xs:complexType>
								<xs:complexContent>
									<xs:extension base="TaxRulePercentOfFees"/>
								</xs:complexContent>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="percentOfRentAndFeesTaxRules" minOccurs="0">
