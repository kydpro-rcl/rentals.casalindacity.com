	<xs:complexType name="Text">
		<xs:annotation>
			<xs:documentation xml:lang="en">A piece of localized text that may or may not have been auto translated.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="textValue" type="xs:string">
