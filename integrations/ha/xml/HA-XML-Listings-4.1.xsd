						<xs:complexType>
							<xs:sequence>
								<xs:element name="unit" type="InternalOnlyUnit" maxOccurs="unbounded"/>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
				</xs:sequence>
			</xs:extension>
