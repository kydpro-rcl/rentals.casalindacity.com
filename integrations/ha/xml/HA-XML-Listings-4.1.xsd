						<xs:element name="appliesToRentAndFeesPerStay" type="AmountAppliesToRentAndFeesPerStay"/>
					</xs:choice>
					<xs:element name="percent" type="xs:decimal"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="TaxRulePercentOfFees">
