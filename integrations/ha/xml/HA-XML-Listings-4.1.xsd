						<xs:element name="appliesPerGuestPerNight" type="AmountAppliesPerGuestPerNight"/>
						<xs:element name="appliesPerGuestPerStay" type="AmountAppliesPerGuestPerStay"/>
						<xs:element name="appliesPerNight" type="AmountAppliesPerNight"/>
						<xs:element name="appliesPerPetPerNight" type="AmountAppliesPerPetPerNight"/>
						<xs:element name="appliesPerPetPerStay" type="AmountAppliesPerPetPerStay"/>
						<xs:element name="appliesPerStay" type="AmountAppliesPerStay"/>
