					<xs:sequence>
						<xs:element name="rule" type="TaxRulePercentOfRentAndFees" maxOccurs="15"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="percentOfRentTaxRules" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="rule" type="TaxRulePercentOfRent" maxOccurs="15"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
